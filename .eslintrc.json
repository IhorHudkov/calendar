{
    "env": {
        "browser": true,
        "es2017": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-tabs":"off",
        "no-undef": "off",
        "no-restricted-syntax": "off",
        "no-bitwise": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "comma-dangle":"off",
        "no-useless-escape":"off",
        "no-alert": "off",
        "class-methods-use-this":"off",
        "no-unused-expressions":"off",
        "prefer-template":"off",
        "no-use-before-define":"off"
    },
    "settings": {
        "import/resolver": "webpack"
    }
}
