{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-tabs":"off",
        "no-undef": "off",
        "no-restricted-syntax": "off",
        "no-bitwise": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off"
    },
    "settings": {
        "import/resolver": "webpack"
    }
}
