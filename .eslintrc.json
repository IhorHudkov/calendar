{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-tabs":"off",
        "no-undef": "off",
        "no-restricted-syntax": "off",
        "no-bitwise": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "comma-dangle":"off",
        "no-useless-escape":"off",
        "no-alert": "off",
        "class-methods-use-this":"off",
        "no-unused-expressions":"off",
        "prefer-template":"off",
        "no-use-before-define":"off",
        "import/prefer-default-export": "off",
        "no-param-reassign":"off",
        "func-names":"off"
    },
    "settings": {
        "import/resolver": "webpack"
    }
}
